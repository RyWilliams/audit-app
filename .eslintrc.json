{
    "extends": "airbnb",
    "rules": {
        "linebreak-style": 0,
        "no-unused-vars": ["error", { "args": "none" }],
        "no-console": "off",
        "max-len": ["error", { "code": 120, "ignoreStrings": true }],
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "browser": true
    }
}